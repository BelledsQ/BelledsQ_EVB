config interface loopback
	option ifname	lo
	option proto	static
	option ipaddr	127.0.0.1
	option netmask	255.0.0.0

config interface lan
	option ifname	eth0
	option type 	bridge
	option proto	static
	option ipaddr	172.16.0.1
	option netmask	255.255.255.0
	option status 	lan

config interface wan
	option proto	dhcp
